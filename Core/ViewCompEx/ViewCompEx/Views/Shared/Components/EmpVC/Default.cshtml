@model IEnumerable<Emp>
<html>
    <head>
        <title> List </title>
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <td> EmpID </td>
                    <td> EmpName </td>
                    <td> DeptName </td>
                    <td> Salary </td>
                </tr>
            @foreach(var temp in Model)
            {
                <tr>
                    <td> @temp.EmpID </td>
                    <td> @temp.EmpName </td>
                    <td> @temp.DeptName </td>
                    <td> @temp.Salary </td>
                </tr>
            }
        </thead>
        </table>
    </body>
</html>
