@model IEnumerable<ClinicRatingVM>

<html>
<head>
    <title> Rating List </title>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <h2 class="title">Rating List</h2>
                        <hr />
                        <table class="table">
                            <thead>
                                <tr align="center">
                                    <th>Sr.No</th>
                                    <th>UserName</th>
                                    <th>Rating</th>
                                    <th>Review</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    int srno = 1;
                                }
                                @foreach (var temp in Model)
                                {
                                    <tr>
                                        <td align="center"> @srno </td>
                                        <td align="center"> @temp.FirstName</td>
                                        <td align="center"> @temp.Rating </td>
                                        <td align="center"> @temp.Review </td>
                                    </tr>
                                    srno++;
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>




