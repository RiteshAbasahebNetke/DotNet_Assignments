@model IEnumerable<DoctorClinicSession>

<html>
<head>
    <title> Rating List </title>
</head>
<body>
    @if (ViewBag.Flag==0){
        <div></div>
    }else{
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <h2 class="title">Session  List</h2>
                            <hr />
                            <table class="table">
                                <thead>
                                    <tr align="center">
                                        <th>Sr.No</th>
                                        <th>StartTime</th>
                                        <th>EndTime</th>
                                        <th> Actions </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        int srno = 1;
                                    }
                                    @foreach (var temp in Model)
                                    {
                                        <tr>
                                            <td align="center"> @srno </td>
                                            <td align="center"> @temp.StartTime</td>
                                            <td align="center"> @temp.EndTime</td>
                                            <td align="center">
                                                <a asp-action="BookApp" asp-route-id="@temp.StartTime" class="btn btn-info"> BookApp </a>
                                            </td>
                                        </tr>
                                        srno++;
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }

</body>
</html>