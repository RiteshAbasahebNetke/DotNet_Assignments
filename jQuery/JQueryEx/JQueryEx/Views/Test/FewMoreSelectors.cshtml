
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FewMoreSelectors</title>
    <script src="~/Scripts/jquery-3.7.1.js"></script>
    <script type="text/javascript">
        $(function(){
            $("[href]").css("background-color", "skyblue");
            $("a[target='_blank']").css("background", "tomato");
            $(":button").css("background-color","yellow")
        });
    </script>
</head>
<body>
    <div> 
        <button> Click </button>
        <input type="button" value="Click" />
        <br />
        <ul>
            <li> First </li>
            <li> Second </li>
            <li> Third </li>
            <li> Fourth </li>
            <li> Fifth </li>
            <li> Sixth </li>
        </ul>
        <br />
        <a target="_self"> First Link </a>
        <a target="_parent"> Second Link </a>
        <a href="#"> Third Link </a>
        <a target="_blank"> Fourth Link </a>
    </div>
</body>
</html>
